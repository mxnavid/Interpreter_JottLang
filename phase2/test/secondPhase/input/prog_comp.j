print("int check");
print(5.0 > 2.0);
print(5 < 2);
print(5 >= 2);
print(5 <= 2);
print(5 == 2);
print(5 != 2);

print("dbl check");
print(5.7 > 2.5);
print(5.7 < 2.5);
print(5.7 >= 2.5);
print(5.7 <= 2.5);
print(5.7 == 2.5);
print(5.7 != 2.5);

print("str check");
print("foo" > "bar");
print("foo" < "bar");
print("foo" >= "bar");
print("foo" <= "bar");
print("foo" == "bar");
print("foo" != "bar");

print("assignment check");
Integer x = 3 > 2;
print(x);
Integer y = 3.0 < 2.0;
print(y);
Integer z = "foo" == "bar";
print(z);
